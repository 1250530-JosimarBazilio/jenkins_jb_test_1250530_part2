            "name": "3. Book CRUD Flow",
            "item": [
                {
                    "name": "3.1 Create Book with Single Author",
                    "event": [
                        {
                            "listen": "prerequest",
                            "script": {
                                "exec": [
                                    "var prefix = '978';",
                                    "var random = '';",
                                    "for (var i = 0; i < 9; i++) {",
                                    "    random += Math.floor(Math.random() * 10);",
                                    "}",
                                    "var isbn12 = prefix + random;",
                                    "var sum = 0;",
                                    "for (var i = 0; i < 12; i++) {",
                                    "    sum += parseInt(isbn12[i]) * (i % 2 === 0 ? 1 : 3);",
                                    "}",
                                    "var checkDigit = (10 - (sum % 10)) % 10;",
                                    "pm.collectionVariables.set('isbn', isbn12 + checkDigit);"
                                ],
                                "type": "text/javascript"
                            }
                        },
                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "pm.test('Book created', function() {",
                                    "    pm.response.to.have.status(201);",
                                    "});",
                                    "",
                                    "var jsonData = pm.response.json();",
                                    "pm.test('ISBN matches', function() {",
                                    "    pm.expect(jsonData.isbn).to.eql(pm.collectionVariables.get('isbn'));",
                                    "});",
